<script type="text/javascript">
    $(function(){
        var height = $(window).height();

        if($('.home_static .sections').css('opacity') < 1){
            opacity = 0.25 + (($(window).scrollTop() / height) / 4 * 3);
            $('.home_static .sections').css('opacity',opacity);
        }
        if($(window).scrollTop() >= height){
            $('.home_static').addClass('reg');
            if(scrollTop < height * 1.5){
                $('.home_static .images .one').css('margin-top', scrollDiff / 2);
                $('.home_static .images .two').css('margin-top', scrollDiff / 3);
            }else{
                $('.home_static .images .one').css('margin-top', 380);
                $('.home_static .images .two').css('margin-top', 285);
            }
        }else{
            $('.home_static').removeClass('reg');
        }

        $('.navbar-nav a').on('click', function(){
            id = $(this).attr('href');
            $('html,body').animate({
                scrollTop: $(id).offset().top + height - 20
            },800);
        });

        if($(window).width() < 992){
            $('#products').removeClass('normal');
        }else{
            $('#products').addClass('normal');
        }

        var iScrollPos = 0;
        $(window).on('scroll', function(){
            var scrollTop = $(this).scrollTop();
            var iCurScrollPos = scrollTop;
            if( $('.home_static .sections').css('opacity') < 1 || iCurScrollPos < iScrollPos ){
                opacity = 0.25 + ((scrollTop / height) / 4 * 3);
                $('.home_static .sections').css('opacity',opacity);
            }else{
                $('.home_static .sections').css('opacity',1);
            }
            if(scrollTop >= height){
                $('.home_static').addClass('reg');


                if(scrollTop < height * 1.5){
                    scrollDiff = scrollTop - height;
                    $('.home_static .images .one').css('margin-top', scrollDiff / 2);
                    $('.home_static .images .two').css('margin-top', scrollDiff / 3);
                }else{

                    if(scrollTop < height * 1.75){
                        scrollDiff = scrollTop - (height * 1.5);

                    }else{
                        if(scrollTop < height * 2.25){
                            scrollDiff = scrollTop - (height * 1.75);

                            $('.home_static #products.normal .women').css('margin-top', scrollDiff / 4);
                            $('.home_static #products.normal .women').css('margin-left', - scrollDiff / 6);

                            $('.home_static #products.normal .men').css('margin-top', - scrollDiff / 4);
                            $('.home_static #products.normal .men').css('margin-left', scrollDiff / 6);

                        }else{
                            if(scrollTop < height * 2.75){
                                scrollDiff = scrollTop - (height * 2.25);
                                $('.home_static #products.normal .women').css('margin-top', 95);
                                $('.home_static #products.normal .women').css('margin-left', - 63);

                                $('.home_static #products.normal .men').css('margin-top', - 95);
                                $('.home_static #products.normal .men').css('margin-left', 63);
                            }
                        }
                    }

                }

            }else{
                $('.home_static').removeClass('reg');
            }
            iScrollPos = iCurScrollPos;
        });
        $(window).on('resize', function(){
            height = $(window).height();
            if($(window).width() < 992){
                $('#products').removeClass('normal');
            }else{
                $('#products').addClass('normal');
            }
        });

        $('.navbar-toggle').on('click', function(){
            $(this).toggleClass("active");
            if($(this).hasClass('collapsed')){
                $('nav').css('background','white');
            }else{
                $('nav').css('background','transparent');
            }
        });

        var scrollPos = 0;
        $('.boxes').on('click', function() {
            scrollPos = $('body').scrollTop();
        });

        $('.close-modal').on('click', function() {
            $('body').animate({scrollTop: scrollPos}, 0);
        });

    });


</script>
